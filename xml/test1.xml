ame" type="string"/><bind calculate=" /data/deviceid " nodeset="/data/user_id" type="string"/><bind calculate="&quot;jHcAAHvZzSjuAaQfT3pm&quot;" nodeset="/data/project_id" type="string"/><bind nodeset="/data/g1/logo" readonly="true()" type="string"/><bind nodeset="/data/g1/cavateco" readonly="true()" type="string"/><bind nodeset="/data/intronote" readonly="true()" type="string"/><bind nodeset="/data/form_o_confirmed" required="true()"/><bind nodeset="/data/tec_name" required="true()" type="string"/><bind nodeset="/data/g2" relevant=" /data/tec_name ='1'"/><bind constraint="regex(., &quot;^[a-zA-Z0-9. _-éáíóãõúçâê]{1,25}$&quot;)" nodeset="/data/g2/tec_given_name" required="true()" type="string"/><bind constraint="regex(., &quot;^[a-zA-Z0-9. _-éáíóãõúçâê]{1,25}$&quot;)" nodeset="/data/g2/tec_family_name" required="true()" type="string"/><bind nodeset="/data/date" required="true()" type="date"/><bind nodeset="/data/prov_id" required="true()" type="string"/><bind nodeset="/data/dist_id" required="true()" type="string"/><bind nodeset="/data/post_id" required="true()" type="string"/><bind nodeset="/data/assoc_id" required="true()" type="string"/><bind calculate=" /data/assoc_id " nodeset="/data/module_id" type="string"/><bind constraint="regex(.,'[0-9]{6}')" nodeset="/data/receipt_id" required="true()" type="string"/><bind constraint=".= /data/receipt_id " jr:constraintMsg="O número do recibo que reentrou não corresponde à sua primeira entrada. Faça a favor de verificar de novo." nodeset="/data/receipt_id_check" required="true()" type="string"/><bind nodeset="/data/occup_type" required="true()" type="string"/><bind nodeset="/data/norm_type" relevant=" /data/occup_type ='customary'" required="true()" type="string"/><bind nodeset="/data/zone_exclusion_y_n" required="true()" type="string"/><bind nodeset="/data/g3" relevant=" /data/zone_exclusion_y_n = 'yes'"/><bind nodeset="/data/g3/zone_overlap" required="true()" type="string"/><bind nodeset="/data/g3/zone_type" required="t